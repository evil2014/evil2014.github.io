<textcomponents>
  <component id="ID_title">
    <text><![CDATA[<b>Comparación de los estándares 802.11</b>]]></text>
  </component>
  <component id="ID_table">
    <text><![CDATA[<table header='none' bordercolor='#98D3D1' borderwidth='3' shadow='false' shadowcolor='#000000' glow='true' glowcolor='#808080' cellpadding='4'>
	<tr>
		<td colspan='4' bgcolor='#0E6360' class="mtable_top"><font size='1' color='#0E6360'><span style="color:#98D3D1">.</span></font></td>
	</tr>
	<tr>
		<td bgcolor='#077A77'  valign="top"><font color='#ffffff' size="11"><b>Estándar IEEE</b></font></td>
		<td bgcolor='#077A77'  valign="top"><font color='#ffffff' size="11"><b>Velocidad máxima</b></font></td>
		<td bgcolor='#077A77'  valign="top"><font color='#ffffff' size="11"><b>Frecuencia</b></font></td>
		<td bgcolor='#077A77'  valign="top"><font color='#ffffff' size="11"><b>Compatibilidad con versiones anteriores</b></font></td>
		
	</tr>

	<tr>
		<td bgcolor='#EFF8F7' valign="top">802.11</td>
		<td bgcolor='#EFF8F7' valign="top">2 Mb/s</td>
		<td bgcolor='#EFF8F7' valign="top">2,4 GHz</td>
		<td bgcolor='#EFF8F7' valign="top">&#8212;</td>
		
		
	</tr>

	   <tr>
		<td bgcolor='#FFFFFF' valign="top">802.11a</td>
		<td bgcolor='#FFFFFF' valign="top">54 Mb/s</td>
		<td bgcolor='#FFFFFF' valign="top">5 GHz</td>
		<td bgcolor='#FFFFFF' valign="top">&#8212;</td>
		
	</tr>
	
	
	<tr>
		<td bgcolor='#EFF8F7' valign="top">802.11b</td>
		<td bgcolor='#EFF8F7' valign="top">11 Mb/s</td>
		<td bgcolor='#EFF8F7' valign="top">2,4 GHz</td>
		<td bgcolor='#EFF8F7' valign="top">&#8212;</td>
		
		
	</tr>

	   <tr>
		<td bgcolor='#FFFFFF' valign="top">802.11g</td>
		<td bgcolor='#FFFFFF' valign="top">54 Mb/s</td>
		<td bgcolor='#FFFFFF' valign="top">2,4 GHz</td>
		<td bgcolor='#FFFFFF' valign="top">802.11b</td>
		
	</tr>
	
	<tr>
		<td bgcolor='#EFF8F7' valign="top">802.11n</td>
		<td bgcolor='#EFF8F7' valign="top">600 Mb/s</td>
		<td bgcolor='#EFF8F7' valign="top">2,4&nbsp;GHz y 5&nbsp;GHz</td>
		<td bgcolor='#EFF8F7' valign="top">802.11a/b/g</td>
		
		
	</tr>

	   <tr>
		<td bgcolor='#FFFFFF' valign="top">802.11ac</td>
		<td bgcolor='#FFFFFF' valign="top">1,3 Gb/s (1300 Mb/s)</td>
		<td bgcolor='#FFFFFF' valign="top">5 GHz</td>
		<td bgcolor='#FFFFFF' valign="top">802.11a/n</td>
		
	</tr>
	
	<tr>
		<td bgcolor='#EFF8F7' valign="top">802.11ad</td>
		<td bgcolor='#EFF8F7' valign="top">7&nbsp;Gb/s (7000&nbsp;Mb/s)</td>
		<td bgcolor='#EFF8F7' valign="top">2,4&nbsp;GHz, 5&nbsp;GHz y 60&nbsp;GHz</td>
		<td bgcolor='#EFF8F7' valign="top">802.11a/b/g/n/ac</td>
		
		
	</tr>

	</table>]]></text>
  </component>
</textcomponents>